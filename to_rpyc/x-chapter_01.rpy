translate id v1_workcafe_340696ad:
    l "Selamat pagi!"

translate id v1_workcafe_b6ee90d6:
    mo "Halo Lena!"

translate id v1_workcafe_9d0f05b4:
    l "Maaf, saya hampir terlambat."

translate id v1_workcafe_14777469:
    mo "Oh, kamu tepat waktu, jadi jangan khawatir."

translate id v1_workcafe_0177231f:
    l "Aku akan ganti baju, dan sebentar lagi aku akan siap."

translate id v1_workcafe_4fa8cae8:
    mo "Tenang saja. Anda adalah kabel listrik!"

translate id v1_workcafe_f42d6a0a:
    "Nyonya Van Dyke sungguh baik padaku. Dia adalah wanita yang cantik."

translate id v1_workcafe_f67818d2:
    l "Siap."

translate id v1_workcafe_40505ce9:
