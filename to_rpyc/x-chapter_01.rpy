translate id _call_label_tutorial_5_b4e35c6a:
    c "Hai teman-teman."

translate id _call_label_tutorial_5_befc931b:
    w "Hai sayang."

translate id _call_label_tutorial_5_5cd76944:
    "Cindy menyapa pacarnya dengan ciuman dan duduk di sebelahnya."

translate id _call_label_tutorial_5_e1272159:
